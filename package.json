{
  "name": "swapy-exchange-electron-wrapper",
  "productName": "Swapy Exchange Electron Wrapper",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "main": "main.js",
  "description": "Electron Wrapper for Swapy Exchange dapp",
  "homepage": "https://swapy.network",
  "author": {
    "name" : "Swapy Network",
    "email" : "contact@swapy.network"
  },
  "scripts": {
    "start": "node setup.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "copy": "cp extensions app/dist -r && cp extensions.js app/dist && cp main.js app/dist"
  },
  "build": {
    "appId": "swapy-exchange-alpha",
    "productName": "Swapy Exchange Alpha",
    "muonVersion": "4.6.7",
    "files": [
      "dist/**/*",
      "!node_modules"
    ],
    "asar": true,
    "asarUnpack": [
      "dist/extensions/"
    ],
    "directories": {
      "output": "dist",
      "app": "app"
    },
    "nodeGypRebuild": false,

    "mac": {
      "category": "your.app.category.type"
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "electron": "git://github.com/brave/electron-prebuilt.git",
    "electron-builder": "^19.52.1",
    "electron-is-dev": "^0.3.0",
    "electron-prebuilt": "github:brave/electron-prebuilt",
    "electron-reload": "^1.2.2"
  }
}
